<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#073781</item>
        <item name="colorPrimaryDark">#062F6D</item>
        <item name="colorAccent">#073781</item>
    </style>

    <style name="showco" parent="@style/Theme.Design.NoActionBar">
        <item name="colorPrimary">#01122C</item>
        <item name="colorPrimaryDark">#01122C</item>
        <item name="colorAccent">#ECF3FD</item>
    </style>

    <style name="home" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">#073781</item>
        <item name="colorPrimaryDark">#062F6D</item>
        <item name="colorAccent">#073781</item>
    </style>

    <style name="FullScreen" parent="@style/Theme.Design.NoActionBar">
        <item name="windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="colorPrimary">#073781</item>
        <item name="colorPrimaryDark">#062F6D</item>
        <item name="colorAccent">#073781</item>
    </style>
</resources>
